{
  "name": "enterprise-ngrx-template",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:enterprise-ngrx-template": "node dist/enterprise-ngrx-template/server/server.mjs",
    "lint": "ng lint",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "e2e:install": "playwright install",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui"
  },
  "prettier": {
    "endOfLine": "auto",
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/animations": "21.0.5",
    "@angular/cdk": "21.0.3",
    "@angular/common": "21.0.5",
    "@angular/compiler": "21.0.5",
    "@angular/core": "21.0.5",
    "@angular/forms": "21.0.5",
    "@angular/material": "21.0.3",
    "@angular/platform-browser": "21.0.5",
    "@angular/platform-server": "21.0.5",
    "@angular/router": "21.0.5",
    "@angular/ssr": "21.0.3",
    "@ngrx/effects": "21.0.0-rc.0",
    "@ngrx/entity": "21.0.0-rc.0",
    "@ngrx/store": "21.0.0-rc.0",
    "@ngrx/store-devtools": "21.0.0-rc.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jose": "^6.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.3",
    "@angular/cli": "^21.0.3",
    "@angular/compiler-cli": "21.0.5",
    "@playwright/test": "^1.50.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.1",
    "@types/node": "^20.17.19",
    "angular-eslint": "21.1.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-boundaries": "^5.3.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jsdom": "^27.1.0",
    "prettier": "^3.7.4",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.47.0",
    "vitest": "^4.0.8"
  }
}
